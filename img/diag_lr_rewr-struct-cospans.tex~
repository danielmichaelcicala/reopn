\[
  \begin{tikzpicture}
   % \draw [help lines, step=0.2, color=blue!10] (-5,-5) grid (5,5); % grid
    % 
    \node (1) at (-1,1) {$ La $};
    \node (4) at (0,1) {$ x $};
    \node (7) at (1,1) {$ La' $};
    \node (2) at (-1,0) {$ Lb $};
    \node (5) at (0,0) {$ y $};
    \node (8) at (1,0) {$ Lb' $};
    \node (3) at (-1,-1) {$ Lc $};
    \node (6) at (0,-1) {$ z $};
    \node (9) at (1,-1) {$ Lc' $};
    \draw [->] (1) to node [] {$  $} (4);
    \draw [->] (7) to node [] {$  $} (4);
    \draw [->] (2) to node [] {$  $} (5);
    \draw [->] (8) to node [] {$  $} (5);
    \draw [->] (3) to node [] {$  $} (6);
    \draw [->] (9) to node [] {$  $} (6);
    \draw [->] (2) to node [] {$  $} (1);
    \draw [->] (2) to node [] {$  $} (3);
    \draw [->] (5) to node [] {$  $} (4);
    \draw [->] (5) to node [] {$  $} (6);
    \draw [->] (8) to node [] {$  $} (7);
    \draw [->] (8) to node [] {$  $} (9);
  \end{tikzpicture}
\]
